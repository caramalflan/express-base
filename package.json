{
  "name": "aetherfe-express",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "devstart": "nodemon ./bin/www",
    "build-css": "sass ./src/stylesheets/styles.scss:./public/stylesheets/styles.css --watch --style compressed",
    "build-js": "yarn run babel ./src/javascripts --watch --out-file ./public/javascripts/app.js",
    "dev": "NODE_ENV=development DEBUG=aetherfe-express:* yarn run npm-run-all -p build-css build-js devstart",
    "prod": "NODE_ENV=production yarn run start",
    "test": "jest"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-react-views": "^0.11.0",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "sass": "^1.29.0"
  }
}
